    "db:init-postgres": "node scripts/init-postgres-db.js",
    "db:use-postgres": "cp .env.postgres .env && npx prisma generate",
    "cleanup:sqlite": "node scripts/cleanup-sqlite.js",
    "db:seed-postgres": "node scripts/seed-postgres.js",
    "db:studio": "npx prisma studio",
    "db:seed-document-types": "node scripts/seed-document-types.js",
    "setup:location-documents": "node scripts/setup-location-document-types.js",
