    "setup:form-fields": "node scripts/setup-form-fields.js",
    "seed:document-types": "node scripts/seed-document-types.js",
    "migrate": "npx prisma migrate dev",
    "migrate:remove-document-field": "npx prisma migrate dev --name remove_document_field",
