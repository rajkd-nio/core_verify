    "seed:nurseio-document-types": "node scripts/seed-nurseio-document-types.js",
    "reset:document-types": "node scripts/reset-and-seed-document-types.js",
    "migrate:new-schema": "node scripts/run-migration.js",
    "db:reset": "node scripts/reset-database.js",
    "db:fresh": "node scripts/create-fresh-db.js",
    "db:fresh-sqlite": "node scripts/create-sqlite-db.js",
    "setup:postgres": "chmod +x scripts/setup-postgres.sh && ./scripts/setup-postgres.sh",
