  "dependencies": {
    "@hookform/resolvers": "^5.0.1",
    "@prisma/client": "^6.6.0",
    "axios": "^1.6.7",
    "better-sqlite3": "^11.9.1",
    "bootstrap": "^5.3.3",
    "dotenv": "^16.4.5",
    "formidable": "^3.5.1",
    "moment": "^2.30.1",
    "next": "^14.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.56.1",
    "reactstrap": "^9.2.2",
    "sqlite3": "^5.1.7",
    "uuid": "^9.0.1",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.1.0",
    "prisma": "^6.6.0"
